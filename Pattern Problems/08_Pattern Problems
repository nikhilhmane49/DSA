#include <iostream>
using namespace std;
int main() {
    
        //******************** star pattern 8*************************
    for(int i=0 ; i<=5 ;i++){
        for(int j=0; j<=i; j++){
            cout<<" ";
        }
        for(int k=0;k < 2 * 5 - 2*i- 1;k++){
            cout<<"*";
        }
        
        cout<<endl;
    }

    return 0;
}

