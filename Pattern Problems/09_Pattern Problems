#include <iostream>
using namespace std;
int main() {

        //******************** star pattern 9*************************
    
       for(int i=1 ; i<=5 ;i++){
        for(int j=0; j<5-i; j++){
            cout<<" ";
        }
        for(int k=0;k < 2 * i - 1;k++){
            cout<<"*";
        }
        
        cout<<endl;
    }
    
    
    for(int i=0 ; i<=5 ;i++){
        for(int j=1; j<=i; j++){
            cout<<" ";
        }
        for(int k=0;k < 2 * 5 - 2*i- 1;k++){
            cout<<"*";
        }
        
        cout<<endl;
    }

    return 0;
}


