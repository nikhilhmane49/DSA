// Online C++ compiler to run C++ program online
#include <iostream>

using namespace std;
int main() {
   
for(int i=0;i<7;i++){
    for(int j=0;j<7;j++){
        if(i%2==0){
            if(i==0 || i==6){
                cout<<"*";
            }
            else{
                if(j==0 || j==6){
                    cout<<"*";
                }
                else{
                    cout<<" ";
                }
            }
        }
        else{
            cout<<" ";
        }
    }
    cout<<endl;
}

    return 0;
}
