#include <iostream>

//******************** star pattern 13*************************



using namespace std;
int main() {
    int num=1;
   for(int i=1;i<=5;i++){
       for(int j=1; j<=i; j++){
           cout<<num<<" ";
              num =num+1;
       }
          cout<<endl;
   }

    return 0;
}
