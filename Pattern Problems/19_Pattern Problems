#include <iostream>
using namespace std;
int main() {
//******************** star pattern 19*************************    
    //print the star 
    for(int i=0;i<5;i++){

for(int j=0;j<5-i;j++){
    cout<<"*";
}
for(int k =0;k<i;k++){
    cout<<" ";
}

for(int k =0;k<i;k++){
    cout<<" ";
}

for(int j=0;j<5-i;j++){
    cout<<"*";
}

cout<<endl;
      
  }
  
  
  for(int i=0;i<5;i++){

for(int j=0;j<=i;j++){
    cout<<"*";
}
for(int k =1;k<5-i;k++){
    cout<<" ";
}

for(int k =1;k<5-i;k++){
    cout<<" ";
}

for(int j=0;j<=i;j++){
    cout<<"*";
}

cout<<endl;
      
  }
    
    return 0;
}


