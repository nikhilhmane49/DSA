#include <iostream>
using namespace std;
int main() {
    
        //******************** star pattern 10*************************
    

       for(int i=1 ; i<=4 ;i++){
        for(int j=1; j<=i; j++){
            cout<<"*";
        }
       
        
        cout<<endl;
    }
    
    
    for(int i=0 ; i<=5 ;i++){
        for(int j=1; j<=5-i; j++){
            cout<<"*";
        }
      
        
        cout<<endl;
    }

    return 0;
}
